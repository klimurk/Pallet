<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:uc="clr-namespace:Pallet.View.SubViews"
                    xmlns:w="clr-namespace:Pallet.View.Windows"
                    xmlns:vm="clr-namespace:Pallet.ViewModels.SubView"
                    xmlns:wvw="clr-namespace:Pallet.ViewModels.Windows">

    <DataTemplate DataType="{x:Type vm:AlarmViewModel}">
        <uc:AlarmView DataContext="{Binding }" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PalletViewModel}">
        <uc:PalletView DataContext="{Binding }" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ManualViewModel}">
        <uc:ManualView DataContext="{Binding }" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LogViewModel}">
        <uc:LogView DataContext="{Binding }" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UsersViewModel}">
        <uc:UsersView DataContext="{Binding }" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type wvw:MainControlViewModel}">
        <uc:MainControl DataContext="{Binding }" />
    </DataTemplate>
</ResourceDictionary>